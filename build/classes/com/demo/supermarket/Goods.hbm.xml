<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
      
<hibernate-mapping>  
    <class name="com.demo.supermarket.Goods">  
        <id name="goodsID">  
            <generator class="uuid"/>  
        </id>  
        <property name="goodsName"/>  
        <property name="goodsCount"/>  
        <property name="goodsPrice"/>  
        <property name="storeWay"/>    
        <property name="sourceArea"/>  
   	 	<property name="setDate"/>  
    	<property name="sellerID"/>  
    	<property name="goodsType"/>  
    	<property name="leastAmount"/>  
    	<property name="largeAmount"/>  
    	<property name="largePrice"/>  
    	<property name="Picture"/>       
    </class>  
</hibernate-mapping> 


