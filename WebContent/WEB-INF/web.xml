<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>supermarket</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>OrderControServlet</servlet-name>
    <servlet-class>com.demo.supermarketSale.OrderControServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderControServlet</servlet-name>
    <url-pattern>/OrderControServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SearchOrderServlet</servlet-name>
    <servlet-class>com.demo.supermarketSale.SearchOrderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchOrderServlet</servlet-name>
    <url-pattern>/SearchOrderServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GoodsControServlet</servlet-name>
    <servlet-class>com.demo.supermarketSale.GoodsControServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoodsControServlet</servlet-name>
    <url-pattern>/GoodsControServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditSellerInfoServlet</servlet-name>
    <servlet-class>com.demo.supermarketSale.EditSellerInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditSellerInfoServlet</servlet-name>
    <url-pattern>/EditSellerInfoServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>ImgUploadServlet</servlet-name>
    <servlet-class>com.demo.supermarketSale.ImgUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImgUploadServlet</servlet-name>
    <url-pattern>/ImgUploadServlet</url-pattern>
  </servlet-mapping>
  
</web-app>